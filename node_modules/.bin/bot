#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/cli/bin/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/cli/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/cli/bin/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/cli/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules/@bot-whatsapp/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/@bot-whatsapp+cli@0.1.33/node_modules:/mnt/c/Cremoto/base-baileys-memory/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bot-whatsapp/cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../@bot-whatsapp/cli/bin/cli.js" "$@"
fi
